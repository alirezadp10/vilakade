{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "8.0.*",
        "alirezadp10/seeder": "^1.0",
        "amidesfahani/nova-persian-date": "^1.0",
        "anhskohbo/no-captcha": "^3.3",
        "ebess/advanced-nova-media-library": "^3.6",
        "fontech/nova-searchable-select-filter": "^0.1.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.75",
        "laravel/horizon": "^5.8",
        "laravel/nova": "^3.30",
        "laravel/sanctum": "^2.14",
        "laravel/telescope": "^4.7",
        "laravel/tinker": "^2.5",
        "laravelwebdev/responsive": "^1.0",
        "maatwebsite/laravel-nova-excel": "^1.2",
        "morilog/jalali": "^3.2",
        "optimistdigital/nova-input-filter": "^1.1",
        "predis/predis": "^2.0",
        "service/kavenegar": "^1.0",
        "silvanite/novafieldcheckboxes": "^1.2",
        "sloveniangooner/searchable-select": "^1.3"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/"
        },
        "classmap": [
            "database/seeders/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./services/Kavenegar"
        }
    ]

}
